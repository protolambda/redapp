{
  "name": "redapp",
  "version": "0.0.1",
  "description": "Redux web3 integration and util for DApps",
  "scripts": {
    "build": "npm run clean && run-p build:**",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env BABEL_ENV=umd NODE_ENV=development rollup -c -o dist/redapp.js",
    "build:umd:min": "cross-env BABEL_ENV=umd NODE_ENV=production rollup -c -o dist/redapp.min.js",
    "clean": "rimraf lib dist es coverage",
    "test": "npm run lint && npm run cover",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:only": "mocha --require babel-core/register --require babel-polyfill --recursive",
    "test:watch": "npm test -- --watch",
    "test:examples-simple": "node examples/simple",
    "cover": "istanbul cover _mocha -- --require babel-core/register --require babel-polyfill --recursive",
    "lint": "eslint src test",
    "prepare": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "main": "lib/index.js",
  "unpkg": "dist/index.min.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/protolambda/redapp.git"
  },
  "keywords": [
    "ethereum",
    "redux",
    "dapp",
    "tracking",
    "transaction",
    "transactions",
    "smartcontract",
    "smartcontracts"
  ],
  "author": "@protolambda <proto@protolambda.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/protolambda/redapp/issues"
  },
  "homepage": "https://github.com/protolambda/redapp#readme",
  "devDependencies": {
    "@babel/cli": "next",
    "@babel/core": "next",
    "@babel/plugin-external-helpers": "next",
    "@babel/plugin-transform-runtime": "next",
    "@babel/preset-env": "next",
    "@babel/preset-stage-2": "next",
    "@babel/runtime": "next",
    "babel-eslint": "^8.2.4",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.1.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.12.0",
    "istanbul": "^1.0.0-alpha",
    "loose-envify": "^1.1.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.63.4",
    "rollup-plugin-babel": "^4.0.0-beta.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "symbol-observable": "^1.2.0"
  },
  "dependencies": {
    "eth-block-tracker": "^4.0.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "uuid": "^3.3.2",
    "web3": "^1.0.0-beta.34",
    "web3-utils": "^1.0.0-beta.34"
  },
  "npmName": "redux",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  }
}
