{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "browsers": [
            "ie >= 11"
          ]
        },
        "exclude": ["transform-async-to-generator", "transform-regenerator"],
        "loose": true,
        "modules": false
      }
    ],
    "@babel/stage-2"
  ],
  "env": {
    "commonjs": {
      "presets": [
        [
          "@babel/env",
          {
            "true": false,
            "modules": "commonjs"
          }
        ],
        "@babel/stage-2"
      ]
    }
  }
}
